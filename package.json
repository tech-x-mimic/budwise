  "main": "index.js",
  "scripts": {
    "build": "npm run sass && npm run prefixer && npm run minify",
    "minify": "cleancss --input-source-map build/budwise.css.map -o build/budwise.min.css build/budwise.css",
    "sass": "node-sass scss/index.scss --output-style expanded --source-map true build/budwise.css",
    "prefixer": "postcss --use autoprefixer --replace build/budwise.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "budwise.css",
    "scss"
  ],
