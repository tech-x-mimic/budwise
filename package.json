  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "clean-css-cli": "^5.2.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1"
  },
  "dependencies": {}
}
